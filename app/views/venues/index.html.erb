<p id="notice"><%= notice %></p>

<div class="boxIndex">
<h2>Venues</h2>
	<% @venues.group_by{ |v| v.name.chars.first }.each do |letter, venues| %>
		<h3><%= letter %></h3>
		<% venues.each do |venue| %>
		<%= link_to venue.name, venue_path(venue) %>
		<% end %>
	<% end %>
</div>

<%= link_to 'New Venue', new_venue_path %>
